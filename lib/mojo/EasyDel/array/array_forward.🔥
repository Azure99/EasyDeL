@always_inline
fn array_relu[T: DType, nelts: Int](inout B: Array[T], A: Array[T]):
    @parameter
    fn v_relu[nelts: Int](i: Int):
        let zeros = SIMD[T, nelts]()
        B.data.simd_store[nelts](
            i,
            (A.data.simd_load[nelts](i) > zeros).cast[T]() * A.data.simd_load[nelts](i),
        )

    vectorize[nelts, v_relu](B.num_elements())


@always_inline
fn array_copy[T: DType, nelts: Int](inout B: Array[T], A: Array[T]):
    memcpy(B.data, A.data, A.num_elements())


@always_inline
fn array_div[
    T: DType, nelts: Int, cores: Int
](inout C: Array[T], A: Array[T], B: Array[T]):
    recursive_broadcast[T, nelts, cores, kernel_div, base_case_div](C, A, B)


@always_inline
fn array_sqrt[T: DType, nelts: Int](inout B: Array[T], A: Array[T]):
    @parameter
    fn v_sqrt[nelts: Int](i: Int):
        let temp = sqrt(A.data.simd_load[nelts](i))
        B.data.simd_store[nelts](i, temp)

    vectorize[nelts, v_sqrt](A.num_elements())


@always_inline
fn array_abs[T: DType, nelts: Int](inout B: Array[T], A: Array[T]):
    @parameter
    fn v_abs[nelts: Int](i: Int):
        let temp = abs(A.data.simd_load[nelts](i))
        B.data.simd_store[nelts](i, temp)

    vectorize[nelts, v_abs](A.num_elements())


@always_inline
fn array_mul[
    T: DType, nelts: Int, cores: Int
](inout C: Array[T], A: Array[T], B: Array[T]):
    recursive_broadcast[T, nelts, cores, kernel_mul, base_case_mul](C, A, B)


@always_inline
fn array_add[
    T: DType, nelts: Int, cores: Int
](inout C: Array[T], A: Array[T], B: Array[T]):
    recursive_broadcast[T, nelts, cores, kernel_add, base_case_add](C, A, B)


@always_inline
fn array_sub[
    T: DType, nelts: Int, cores: Int
](inout C: Array[T], A: Array[T], B: Array[T]):
    recursive_broadcast[T, nelts, cores, kernel_sub, base_case_sub](C, A, B)


# More Automated Functions


@always_inline
fn array_pow[T: DType, nelts: Int, cores: Int](A: Array[T], B: Array[T]) -> Array[T]:
    var C: Array[T] = Array[T](B.array_shape)
    C.alloc(0.0)
    array_pow[T, nelts, cores](C, A, B)
    return C


