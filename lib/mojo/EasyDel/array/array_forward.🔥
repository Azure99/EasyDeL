@always_inline
fn matmul[T: DType, nelts: Int, cores: Int](A: Array[T], B: Array[T]) -> Array[T]:
    var C: Array[T] = Array[T](A, B)
    recursive_broadcast[T, nelts, cores, kernel_matmul, base_case_matmul](C, A, B)
    return C


@always_inline
fn matmul[T: DType, cores: Int = 1](A: Array[T], B: Array[T]) -> Array[T]:
    var C: Array[T] = Array[T](A, B)
    recursive_broadcast[T, Array[T].nelts, cores, kernel_matmul, base_case_matmul](
        C, A, B
    )
    return C


